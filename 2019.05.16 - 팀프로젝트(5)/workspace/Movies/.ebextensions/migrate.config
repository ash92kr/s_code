container_commands:
    01_migrate:
        command: "python manage.py migrate"
        leader_only: true
    02_chown_sqlitedb:
        command: "sudo chown wsgi db.sqlite3"
        leader_only: true
    03_collectstatic:
        command: "python manage.py collectstatic"
    04_superuser:
        command: "python manage.py loaddata users.json"
        leader_only: true